{
    "private": true,
    "name": "volmex-amm",
    "version": "1.0.0",
    "license": "GPL-3.0-only",
    "description": "Volmex AMM",
    "scripts": {
        "compile": "npx hardhat compile",
        "testrpc": "ganache-cli --deterministic --gasLimit 10000000",
        "test": "npx hardhat test",
        "coverage": "npx hardhat coverage",
        "format": "yarn prettier --write contracts/**/*.sol && yarn prettier --write test/**/*.ts && yarn prettier --write scripts/**/*.js",
        "lint": "eslint .",
        "lint:contracts": "solhint contracts/*.sol",
        "pool": "npx hardhat run --network $NETWORK scripts/createPool.ts",
        "pool:2x": "npx hardhat run --network $NETWORK scripts/2xPool.ts",
        "upgrade": "npx hardhat run --network $NETWORK scripts/upgrade.ts"
    },
